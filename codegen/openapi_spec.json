  "paths": {
    "/features": {
      "get": {
        "operationId": "getFeatures",
        "summary": "Get features",
        "description": "Get all features",
        "tags": [
          "Features"
        ],
        "parameters": [
          {
            "name": "q",
            "in": "query",
            "description": "Sub-string to match against feature name or ID",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "updated_since",
            "in": "query",
            "description": "UTC timestamp (in ISO8601 format) that the updated_at field must be larger than.",
            "required": false,
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "tag",
            "in": "query",
            "description": "A string tag value.",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "assigned_to_user",
            "in": "query",
            "description": "The ID or email address of user to return assigned features for.",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "page",
            "in": "query",
            "description": "A specific page of results.",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          },
          {
            "name": "per_page",
            "in": "query",
            "description": "Number of results per page.",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "A list of features",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/FeaturesResponse"
                }
              }
            }
          }
        }
      }
    },
